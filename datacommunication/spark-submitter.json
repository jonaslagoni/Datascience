{
	"asyncapi": "2.0.0",
	"info": {
		"title": "Spark submitter application",
		"version": "1.0.0",
		"license": {
			"name": "Apache 2.0",
			"url": "https://www.apache.org/licenses/LICENSE-2.0"
		}
	},
	"defaultContentType": "application/json",
	"channels": {
		"datascience/processed/status": {
			"description": "Publishes when new data is being processed",
			"publish": {
				"message": {
					"$ref": "./components/messages/statusMessage.json"
				}
			}
		},
		"datascience/warehouse/data": {
			"description": "subscribes to event for when new data is being processed",
			"subscribe": {
				"message": {
					"$ref": "./components/messages/rawEnergyDataMessage.json"
				}
			}
		}
	}
}
